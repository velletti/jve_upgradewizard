{
    "name": "jvelletti/jve-upgradewizard",
    "type": "typo3-cms-extension",
    "version": "12.4.9",
    "description": "Update helper for lts12 Fixes the ways Typoscript and TSconfig included files have been added in database.",

    "keywords": [
        "typoscript",
        "upgrade",
        "LTS12",
        "template",
        "TYPO3"
    ],
    "authors": [
        {
            "name": "Joerg Velletti",
            "role": "Developer",
            "homepage": "http://www.velletti.de/"
        }
    ],
    "require": {
        "typo3/cms-core": "^11.5 || ^12.4"
    },
    "license": "mit",
    "autoload": {
        "psr-4": {
            "Jvelletti\\JveUpgradewizard\\": "Classes"
        }
    },
    "replace": {
        "typo3-ter/jve-upgradewizard": "self.version"
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "jve_upgradewizard"
        }
    }
}